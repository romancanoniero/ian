        <action
            android:id="@+id/action_otpVerificationActivity_to_pinSetupFragment"
            app:destination="@id/pinSetupFragment" />
        <action
            android:id="@+id/action_otpVerificationActivity_to_speedDialSetupFragment"
            app:destination="@id/speedDialSetupFragment" />
        <action
            android:id="@+id/action_otpVerificationActivity_to_locationRequiredFragment"
            app:destination="@id/locationRequiredFragment" />
        <action
            android:id="@+id/action_otpVerificationActivity_to_addContactsFromPhoneFragment"
            app:destination="@id/addContactsFromPhoneFragment" />
        <action
            android:id="@+id/action_otpVerificationActivity_to_homeFragment"
            app:destination="@id/homeFragment" />
