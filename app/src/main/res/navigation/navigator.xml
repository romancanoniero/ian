            app:destination="@id/eventTypeExplanationDialog">

            <argument
                android:name="eventType"
                app:argType="com.iyr.ian.enums.EventTypesEnum" />
            <argument
                android:name="dialogType"
                app:argType="com.iyr.ian.ui.dialogs.DialogFunctionEnum" />


        </action>
    </fragment>
