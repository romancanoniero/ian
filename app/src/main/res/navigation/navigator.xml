            app:argType="string" />
        <action
            android:id="@+id/action_otpVerificationActivity_to_setupFragment"
            app:destination="@id/setupFragment" />
        <action
            android:id="@+id/action_otpVerificationActivity_to_pressOrTapSetupFragment"
