        android:id="@+id/notificationsFragment"
        android:name="com.iyr.ian.ui.notifications.NotificationsFragment"
        android:label="NotificationsFragment"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/otpVerificationActivity"
        android:name="com.iyr.ian.ui.otp.OtpVerificationFragment"
        android:label="OtpVerificationActivity"
        tools:layout="@layout/activity_otp_verification">
