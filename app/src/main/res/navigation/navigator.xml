            android:id="@+id/action_setupFragment_to_addContactsFromPhoneFragment"
            app:destination="@id/addContactsFromPhoneFragment" />
        <action
            android:id="@+id/action_setupFragment_to_locationRequiredFragment"
            app:destination="@id/locationRequiredFragment" />
        <action
            android:id="@+id/action_setupFragment_to_pressOrTapSetupFragment"
            app:destination="@id/pressOrTapSetupFragment" />
    </fragment>
