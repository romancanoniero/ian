            android:id="@+id/action_homeFragment_to_eventTypeSelectorFragment"
            app:destination="@id/eventTypeSelectorFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mapSituationFragment"
            app:destination="@id/mapSituationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_friendsFragment"
            app:destination="@id/friendsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <argument
            android:name="user"
            app:argType="com.iyr.ian.dao.models.User" />
        <action
            android:id="@+id/action_homeFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_contactsFragment"
            app:destination="@id/contactsFragment" />
        <argument
            android:name="firstRun"
            app:argType="boolean"
            />
    </fragment>
    <fragment
